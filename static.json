{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@render-inc/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/resetpassword",
        "destination": "/index.html",
        "status": 301
      }
    ]
  }
  